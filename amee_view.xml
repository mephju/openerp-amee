<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
	    
	    
	    <!-- NEW MAIN MENU ITEM LWC-EPI -->
	    <record model="ir.actions.act_window" id="action_amee_form">
			<field name="name">LWC-EPI</field>
			<field name="res_model">amee.config</field>
			<field name="view_type">form</field>
		</record>
		
		<menuitem name="LWC-EPI" icon="STOCK_OPEN" id="amee_menu"   web_icon="images/amee.png" web_icon_hover="images/amee-hover.png" action="action_amee_form"/>

		<record model="ir.ui.view" id="amee_form_view">
			<field name="name">amee.form</field>
			<field name="model">amee.config</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="amee">
					<field name="api_key" />
					<field name="password" />
					<field name="url" />
				</form>
			</field>
		</record>
		
		
		<record model="ir.ui.view" id="amee_form_view">
			<field name="name">amee.tree</field>
			<field name="model">amee.config</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<form string="amee">
					<field name="api_key" />
					<field name="password" />
					<field name="url" />
				</form>
			</field>
		</record>




	</data>

</openerp>